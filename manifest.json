{
	"name": "TimeOut",
	"description": "An app to help you become more productive!",
	"version": "1.0",
	"icons": {"128": "stopwatch1282.png"},
	"background": {"scripts": ["background.js"], "persistent": true},
	"options_page": "options.html",
	"permissions": ["activeTab", "storage", "tabs"],
	"browser_action": {"default_popup": "popup.html", "default_icon": {"128": "stopwatch1282.png"}},
	"manifest_version": 2
}